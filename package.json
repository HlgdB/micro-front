{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.3",
    "antd": "^4.6.6",
    "axios": "^0.19.2",
    "axios-react": "^2.0.2",
    "js-cookie": "^2.2.1",
    "konva": "^7.2.2",
    "lint-staged": "^10.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-konva": "^17.0.1-3",
    "swiper": "^6.1.2",
    "umi": "^3.2.3",
    "use-image": "^1.0.7",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.4.11",
    "eslint": "^7.17.0",
    "prettier": "^2.2.1"
  }
}
